#!/usr/bin/env bash

## pysetup.sh: setup environment variables for Python setup

## $Revision$
## Copyright 2011 Michael M. Hoffman <mmh1@uw.edu>

set -o nounset
set -o pipefail
set -o errexit

if [[ $# != 0 || "$1" == "--help" || "$1" == "-h" ]]; then
    echo usage: "$0"
    exit 2
fi

if [ "$CFLAGS" ]; then
    CFLAGS="$CFLAGS -fno-strict-aliasing"
fi

"$0"
